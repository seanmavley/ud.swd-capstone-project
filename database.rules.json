{
  "rules": {
    "codes": {
      ".read": "true",
      ".write": "auth.uid != null",
      "$uid": {
        "snippets": {
          "$lang": {
            ".write": "newData.child('uid').val() == auth.uid"
          }
        }
      }
    },
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    },
    "languages": {
      ".write": "auth.uid != null && auth.email == 'seanmavley@gmail.com'",
      ".read": "true"
    },
    "revision": {
      ".write": "true",
      ".read": "true"
    }
  }
}